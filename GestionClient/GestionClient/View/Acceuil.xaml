<UserControl x:Class="GestionClient.Accueil"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestionClient"        
        mc:Ignorable="d">
    <Grid>

        <ListView Name="lstCabinet" HorizontalAlignment="Stretch" ItemsSource="{Binding CabinetList}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Raison Sociale" DisplayMemberBinding="{Binding RaisonSociale}"/>
                    <GridViewColumn Header="Praticiens"  />
                    <GridViewColumn Header="Adresse" />
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</UserControl>
